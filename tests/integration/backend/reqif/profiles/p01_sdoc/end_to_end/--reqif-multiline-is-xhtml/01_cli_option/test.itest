RUN: %strictdoc export --formats=reqif-sdoc --reqif-multiline-is-xhtml %S/sample.sdoc
RUN: %strictdoc import reqif sdoc %S/output/reqif/output.reqif %S/output/
RUN: %diff %S/sample.sdoc %S/output/sample.sdoc

RUN: %cat %S/output/reqif/output.reqif | filecheck %s --check-prefix CHECK-EXPORTED-REQIF
CHECK-EXPORTED-REQIF:<DATATYPE-DEFINITION-STRING IDENTIFIER="SDOC_DATATYPE_SINGLE_LINE_STRING"/>
CHECK-EXPORTED-REQIF:<DATATYPE-DEFINITION-XHTML IDENTIFIER="SDOC_DATATYPE_MULTI_LINE_STRING"/>

CHECK-EXPORTED-REQIF:      <SPEC-OBJECT-TYPE IDENTIFIER="FREE_TEXT_{{.*}}" LONG-NAME="FREETEXT">
CHECK-EXPORTED-REQIF-NEXT:   <SPEC-ATTRIBUTES>
CHECK-EXPORTED-REQIF-NEXT:     <ATTRIBUTE-DEFINITION-XHTML IDENTIFIER="ReqIF.Text" LONG-NAME="ReqIF.Text">
CHECK-EXPORTED-REQIF-NEXT:       <TYPE>
CHECK-EXPORTED-REQIF-NEXT:         <DATATYPE-DEFINITION-XHTML-REF>SDOC_DATATYPE_MULTI_LINE_STRING</DATATYPE-DEFINITION-XHTML-REF>
CHECK-EXPORTED-REQIF-NEXT:       </TYPE>
CHECK-EXPORTED-REQIF-NEXT:     </ATTRIBUTE-DEFINITION-XHTML>
CHECK-EXPORTED-REQIF-NEXT:   </SPEC-ATTRIBUTES>
CHECK-EXPORTED-REQIF-NEXT: </SPEC-OBJECT-TYPE>

CHECK-EXPORTED-REQIF:             <ATTRIBUTE-VALUE-XHTML>
CHECK-EXPORTED-REQIF:              <DEFINITION>
CHECK-EXPORTED-REQIF:                <ATTRIBUTE-DEFINITION-XHTML-REF>ReqIF.Text</ATTRIBUTE-DEFINITION-XHTML-REF>
CHECK-EXPORTED-REQIF:              </DEFINITION>
CHECK-EXPORTED-REQIF:              <THE-VALUE>Document free text.</THE-VALUE>
CHECK-EXPORTED-REQIF:            </ATTRIBUTE-VALUE-XHTML>

CHECK-EXPORTED-REQIF:                 <ATTRIBUTE-VALUE-XHTML>
CHECK-EXPORTED-REQIF-NEXT:              <DEFINITION>
CHECK-EXPORTED-REQIF-NEXT:                <ATTRIBUTE-DEFINITION-XHTML-REF>ReqIF.Text</ATTRIBUTE-DEFINITION-XHTML-REF>
CHECK-EXPORTED-REQIF-NEXT:              </DEFINITION>
CHECK-EXPORTED-REQIF-NEXT:              <THE-VALUE>Section free text.</THE-VALUE>
CHECK-EXPORTED-REQIF-NEXT:            </ATTRIBUTE-VALUE-XHTML>

CHECK-EXPORTED-REQIF:                 <ATTRIBUTE-VALUE-XHTML>
CHECK-EXPORTED-REQIF-NEXT:              <DEFINITION>
CHECK-EXPORTED-REQIF-NEXT:                <ATTRIBUTE-DEFINITION-XHTML-REF>ReqIF.Text</ATTRIBUTE-DEFINITION-XHTML-REF>
CHECK-EXPORTED-REQIF-NEXT:              </DEFINITION>
CHECK-EXPORTED-REQIF-NEXT:              <THE-VALUE>Statement line #1
CHECK-EXPORTED-REQIF-EMPTY:
CHECK-EXPORTED-REQIF-NEXT:                Statement line #2
CHECK-EXPORTED-REQIF-EMPTY:
CHECK-EXPORTED-REQIF-NEXT:                Statement line #3</THE-VALUE>
