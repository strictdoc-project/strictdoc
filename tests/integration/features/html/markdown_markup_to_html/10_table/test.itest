RUN: %strictdoc export %S --formats=html --output-dir %T | filecheck %s --dump-input=fail
CHECK: Published: Markdown table document

RUN: %cat %T/html/10_table/input.html | filecheck %s --dump-input=fail --check-prefix CHECK-HTML-FILE
CHECK-HTML-FILE: <table>
