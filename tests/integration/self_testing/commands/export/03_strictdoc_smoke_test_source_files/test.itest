REQUIRES: PLATFORM_IS_NOT_WINDOWS

RUN: cd %strictdoc_root && %strictdoc export "%strictdoc_root/docs" --output-dir="%S/Output" --experimental-enable-file-traceability

RUN: %check_exists --file "%S/Output/html/index.html"
