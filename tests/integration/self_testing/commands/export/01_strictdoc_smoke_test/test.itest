RUN: %check_exists --file "%S/Output/html/docs/strictdoc_20_L1_Open_Requirements_Tool-TABLE.html"
RUN: %check_exists --file "%S/Output/html/docs/strictdoc_20_L1_Open_Requirements_Tool-TRACE.html"
