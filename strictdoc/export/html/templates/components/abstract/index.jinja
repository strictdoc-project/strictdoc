<a name="_TOP"></a>

{%- if document.free_texts -%}
  {%- for free_text in document.free_texts %}
    <div class="free_text" data-testid="document-abstract">
      <sdoc-autogen>{{ renderer.render_free_text(document_type, free_text) }}</sdoc-autogen>
    </div>
  {%- endfor -%}
{%- elif not document.section_contents -%}
  <sdoc-main-placeholder data-testid="document-root-placeholder">
    The document is empty.
    <br/>Start adding text, sections, and requirements.
  </sdoc-main-placeholder>
{%- endif -%}
