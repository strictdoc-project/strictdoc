RUN: %strictdoc export %S --formats=html --output-dir %T | filecheck %s --dump-input=fail
CHECK: Published: Document example

RUN: %cat %T/html/%THIS_TEST_FOLDER/input.html | filecheck %s --dump-input=fail --check-prefix CHECK-HTML-FILE

CHECK-HTML-FILE: show-node-type-name="REQUIREMENT"
CHECK-HTML-FILE: data-path="../#REQ-1"
CHECK-HTML-FILE: id="REQ-1"
CHECK-HTML-FILE: data-uid="REQ-1"
CHECK-HTML-FILE: <sdoc-node-field-label>Rationale:</sdoc-node-field-label>
