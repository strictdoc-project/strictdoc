<div class="pdf-toc" data-testid="pdf-toc-list">
    {%- for item in document_iterator.table_of_contents() -%}

      <div class="pdf-toc-row" data-nodeid="{{ item.mid.get_string_value() }}">

          <span class="pdf-toc-cell">
            {{ item.context.title_number_string if item.context.title_number_string else "&nbsp;" * (item.ng_level * 2 - 1) }}
          </span>
          <span dotted class="pdf-toc-cell">
            <a href="#{{ link_renderer.render_local_anchor(item) }}" data-turbo="false">
              {%- if item.reserved_title is not none -%}
                {{ item.reserved_title }}
              {%- endif -%}
            </a>
          </span>
          <span page-number class="pdf-toc-cell">(••)</span>

      </div>{# /.pdf-toc-row #}

    {%- endfor %}
</div>{# /.pdf-toc #}
